<TriplesMapMonumento>
	rr:logicalTable [ 
        rr:tableName  "FARMACIA" # https://www.zaragoza.es/sede/servicio/farmacia.csv
    ];
	
    rr:subjectMap [    
		a rr:Subject; rr:termType rr:IRI; rr:class schema:Pharmacy;
		rr:column "ID";
	];

	rr:predicateObjectMap [ 
      rr:predicateMap [ rr:constant schema:name ]; 
      rr:objectMap    [ rr:termType rr:Literal; rr:column "TITLE"; ]; 
    ];

.